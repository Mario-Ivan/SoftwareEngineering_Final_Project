
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User{
  id Int @id @default(autoincrement())
  firstName String 
  lastName String
  password String 
  email String @unique
  telepon String @unique
  profile String?
}